/*Tento algoritmus najde najvacsi spolocny delitel*/

int main ()
{
    int a,b;
    printf ("Zadaj cislo a: ");
    scanf ("%d", &a);
    printf ("Zadaj cislo b: ");
    scanf ("%d", &b);
    int c = a;
    int d = b;
    while (a != b){
        if (a>b){
            a=a-b;
        }
        if (b>a){
            b=b-a;  
        }
      
    }
    printf ("Najvacsi spolocny delitel (%d, %d) je %d.\n", c, d, a);
    
    return 0;
}
